{
    "plugins": ["plugins/markdown"],
    "source": {
        "include":["src"],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true
    },
    "opts":{
        "recurse":true,
        "destination":"./out/"
    }
}